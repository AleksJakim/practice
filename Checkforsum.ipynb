    "def is_sum(arr, n):\n",
    "\n",
    "    for i in range(0, len(arr)):\n",
    "        if n - arr[i] in arr:\n",
    "            return True\n",
    "    else:\n",
    "            return False   \n",
    "\n",
    "A = [1, 4, 49, 6, 51, 8]\n",
    "n = 100\n",
    "print(is_sum(A,n))"
